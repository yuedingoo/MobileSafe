<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yueding.mobilesafe">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_CONTACTS"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".shezhi.SettingActivity" />
        <activity android:name=".fangdao.SetupOverActivity" />
        <activity android:name=".fangdao.Setup1Activity" />
        <activity android:name=".fangdao.Setup2Activity" />
        <activity android:name=".fangdao.Setup3Activity" />
        <activity android:name=".fangdao.Setup4Activity" />
        <activity android:name=".fangdao.ContactsActivity"></activity>
    </application>

</manifest>